import multiprocessing

import time
def wait():
    time.sleep(0.5)
    print("Done waiting")

process = multiprocessing.Process(target=wait)

# Add code here
process.start()
print("Finished")

process.join()

import time
def wait():
    time.sleep(0.5)
    print("Done waiting")

process = multiprocessing.Process(target=wait)

# Add code below
process.start()
process.join()
print("Finished")

import time
def wait():
    time.sleep(0.5)
    print("Done waiting")

# Add code below
start = time.time()

p1 = multiprocessing.Process(target=wait)
p2 = multiprocessing.Process(target=wait)

p1.start()
p2.start()

p1.join()
p2.join()

end = time.time()

elapsed = end - start

import time
def wait():
    time.sleep(0.5)
    print("Done waiting")

# Add code below
start = time.time()

processes = [multiprocessing.Process(target=wait) for _ in range(6)]

for process in processes:
    process.start()

for process in processes:
    process.join()

end = time.time()

elapsed = end - start
